create table carro (
	id serial primary key,
	ano int,
	marca varchar(20),
	modelo varchar (20)
	

);



insert into carro (ano, marca, modelo)
values (1005 , 'porsche', '70');

insert into carro(marca,modelo)values('renault', 'uno');

update carro set
ano = 2000
where id=7;

select * from carro;


delete from carro;
